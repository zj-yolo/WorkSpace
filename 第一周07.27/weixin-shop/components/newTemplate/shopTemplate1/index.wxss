.c_brand {
  padding: 20rpx;
  width: 100%;
  height: 100vh;
  box-sizing: border-box;
}

.storeTitle {
  font-size: 32rpx;
  font-weight: 800;
}

.c_tabcontent {
  border-bottom: 1rpx solid #e5e5e5;
  padding: 20rpx 0;
  display: flex;
  width: 100%;
  box-sizing: border-box;
}

.c_tabcontentRight {
  width: 75%;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 32rpx;
  margin-top: 10rpx;
}

.c_content {
  width: 76%;
}

.storeName {
  font-size: 30rpx;
}

.hasadd {
  margin: 10rpx 0;
  display: flex;
  align-items: center;
}

.eye {
  width: 42rpx;
  height: 28rpx;
}

.c_hasadd {
  margin-left: 10rpx;
  font-size: 22rpx;
  color: #848484;
}

.desc {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  font-size: 25rpx;
}

.arrowImage {
  width: 18rpx;
  height: 32rpx;
}

.c_tabcontent:last-child {
  border-bottom: none;
}
.logoImg{
  width: 100rpx;
  height: 100rpx;
}
.logoImg image{
  width: 100%;
  height: 100%;
