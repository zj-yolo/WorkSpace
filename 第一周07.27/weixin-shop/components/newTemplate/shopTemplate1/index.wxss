.header {
  background-color: #8b0000;
  height: 168rpx;
  width: 100%;
  padding: 5rpx 20rpx 0;
  box-sizing: border-box;
}

.search {
  display: flex;
  align-items: center;
}

.searchItem {
  display: flex;
  position: relative;
  width: calc(100% - 40rpx - 40rpx - 60rpx);
}

.searchinput {
  height: 66rpx;
  width: 100%;
  background-color: #fff;
  border-radius: 30rpx;
  padding-left: 68rpx;
}

.searchIcon {
  width: 26rpx;
  height: 25rpx;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}

.cateItem {
  display: flex;
  margin-left: 15rpx;
}

.cateIcon {
  width: 39rpx;
  height: 35rpx;
}

.cateText {
  margin-left: 10rpx;
  font-size: 28rpx;
  color: #fff;
}

.nav {
  display: flex;
  margin-top: 10rpx;
}

.navItem {
  color: #f5f5f5;
  font-size: 30rpx;
  padding: 15rpx 0;
  margin: 0 15rpx;
}

.navItemActive {
  border-bottom: 5rpx solid #fff;
  padding-bottom: 10rpx;
  font-weight: 700;
}

.live {
  height: 268rpx;
  width: 100%;
  background-color: #8b0000;
  /* background-image: url(/images/newtemplate/livebg.png); */
  background-size: 100% 100%;
  background-repeat: no-repeat;
  display: flex;
  padding: 24rpx 20rpx 24rpx 26rpx;
  box-sizing: border-box;
}

.liveImg {
  width: 220rpx;
  height: 220rpx;
  border-radius: 10rpx;
}

.liveRight {
  margin-left: 30rpx;
  flex: 1;
  width: 100%;
}

.liveTop {
  display: flex;
  width: 100%;
  justify-content: space-between;
}

.livestatus {
  color: #fff;
  font-size: 22rpx;
  text-align: center;
  width: 105rpx;
  height: 34rpx;
  background-image: linear-gradient(270deg,
      #ff3760 0%,
      #ff8787 100%);
  border-radius: 10rpx;
}

.livestatusIcon {
  margin-right: 5rpx;
  width: 18rpx;
  height: 16rpx;
}

.liveAdd {
  font-size: 30rpx;
  color: #fff;
  letter-spacing: 2rpx;
  font-weight: 700;
  width: 250rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.towatch {
  width: 122rpx;
  height: 46rpx;
  background-color: #ffffff;
  border-radius: 40rpx;
  color: #c12020;
  font-size: 22rpx;
  text-align: center;
  line-height: 46rpx;
}

.livegoods {
  margin-top: 25rpx;
  display: flex;
}

.livegoodsItemImg {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.livegoodsItem {
  position: relative;
  width: 104rpx;
  height: 104rpx;
  margin-right: 10rpx;
}

.livegoodsItemText {
  color: #fff;
  font-size: 20rpx;
  height: 28rpx;
  width: 100%;
  text-align: center;
  position: absolute;
  left: 0;
  bottom: 2rpx;
  background: rgba(0, 0, 0, .3)
}

.coupon {
  background: #fff;
  height: 180rpx;
  padding: 15rpx 0 15rpx 10rpx;
  box-sizing: border-box;
  width: 100%;
}

.couponList {
  width: 100%;
  height: 100%;
  white-space: nowrap;
}

.couponItem {
  display: inline-block;
  background: #cf2222;
  height: 100%;
  width: 393rpx;
  border-radius: 10rpx;
  margin-right: 10rpx;
  position: relative;
}

.circle {
  background: #fff;
  width: 20rpx;
  height: 20rpx;
  border-radius: 50%;
}

.topcircle {
  position: absolute;
  right: 85rpx;
  top: -8rpx;
}

.bottomcircle {
  position: absolute;
  right: 85rpx;
  bottom: -8rpx;
}

.whiteline {
  position: absolute;
  -webkit-writing-mode: vertical-rl;
  writing-mode: vertical-rl;
  color: #fff;
  letter-spacing: 4rpx;
  bottom: 30rpx;
  right: 72rpx;
  font-size: 26rpx;
}

.couponcontent {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx 15rpx;
  box-sizing: border-box;
  color: #fff;
  letter-spacing: 2rpx;
}
.fristImg{
  display:block;
  width: 100%;
  height: 100%;
}
.secondImg{
  display:block;
  width: 100%;
}
.Itemtitle{
  background: #fff;
  height: 160rpx;
  padding-top: 40rpx;
  text-align: center;
  box-sizing: border-box;
}
.Itemgoods{
  background: #f5f5f5;
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  padding: 15rpx 10rpx;
  box-sizing: border-box;
}
.goodsContent{
  width: 360rpx;
  height: 518rpx;
  margin-right: 10rpx;
  margin-bottom: 10rpx;
}
.goodsContent:nth-of-type(even){
  margin-right: 0;
}
.goodsImgs{
  width: 363rpx;
  height: 363rpx;
  display:block;
}
.goodsInfos{
  height: 158rpx;
  background: #fff;
  padding: 5rpx 18rpx 15rpx 13rpx;
  width: 100%;
  box-sizing: border-box;
}
.goodsInfo-name{
  font-size: 28rpx;
  min-height: 66rpx;
  width: 100%;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
}
.goodsInfo-price{
  font-size: 28rpx;
  color: #d52121;
  margin-top: 15rpx;
}
.cateitem{
 margin: 20rpx;
}
.cateactive{
  color: #8b0000;
}
