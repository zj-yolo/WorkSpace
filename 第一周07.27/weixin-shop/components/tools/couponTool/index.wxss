.cc_couponList {
  margin:20rpx 0;
  width: 100%;
  white-space: nowrap;
}

.cc_couponItem {
  margin-left: 20rpx;
  display: inline-flex;
  align-items: center;
  background: #f4f4f4;
}
.cc_ashy{
  background-color: #cccccc !important;
}
.cc_title{
  margin-bottom:7rpx;letter-spacing:1rpx
}
.cc_titleashy{
  color: #c9c9c9!important;
}
.cc_left {
  background: #ffa200;
  color: #fff;
  text-align: center;
  padding: 20rpx 10rpx;
  box-sizing: border-box;
}

.cc_right {
  height: 100%;
  width: 90rpx;
  font-size: 25rpx;
  padding: 0 10rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.cc_receive {
  font-size: 16rpx;
  padding: 5rpx;
  box-sizing: border-box;
  background: #ffa200;
  color: #fff;
  border-radius: 20rpx;
}