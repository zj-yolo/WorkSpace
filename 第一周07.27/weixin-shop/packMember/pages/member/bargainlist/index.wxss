.box {
  background: #f5f5f5;
  width: 100%;
  position: absolute;
  top: 0;
  left: 0;
  height: 100%;
}

.title {
  margin-top: 1rpx;
  background-color: #fff;
  width: 100%;
  height: 75rpx;
  line-height: 75rpx;
  font-size: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}

.cate {
  width: 50%;
  text-align: center;
}

.cate text {
  display: inline-block;
  padding: 0 25rpx;
}

.activeItem {
  border-bottom: 3rpx solid #ff3334;
  color: #ff3334;
}

.content {
  margin-top: 15rpx;
}

.item {
  background-color: #fff;
  padding: 30rpx 30rpx;
  width: 100%;
  box-sizing: border-box;
  margin-bottom: 15rpx;
}

.goods {
  display: flex;
  height: 200rpx;
}

.goodsimages {
  position: relative;
}

.label {
  width: 72rpx;
  height: 25rpx;
  font-size: 18rpx;
  box-sizing: border-box;
  border-radius: 0 50rpx 50rpx 0;
  color: #fff;
  background-color: #ff0101;
  text-align: center;
  position: absolute;
  top: 10rpx;
}

.labelaleardy {
  background-color: #d5d5d5;
  color: #fff;
}

.goodsimages image {
  width: 200rpx;
  height: 200rpx;
}

.goodsright {
  margin-left: 28rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  width: cale(100% - 220rpx - 28rpx - 30rpx * 2);
  box-sizing: border-box;
  overflow: hidden;
}

.itemtitle {
  margin-top: 10rpx;
}

.goodstitle {
  display: -webkit-box;
  overflow: hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  font-size: 30rpx;
  color: #060606;
}

.subtitle {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 100%;
  margin-top: 10rpx;
  font-size: 24rpx;
  color: #b4b4b4;
}

.specs {
  display: flex;
  align-items: flex-end;
  justify-content: space-between;
}

.specs text {
  color: #fd402d;
  font-size: 30rpx;
}

.goodsnumber {
  font-size: 20rpx;
  color: #6b6b6b;
}

.line {
  margin-top: 30rpx;
  height: 2rpx;
  background-color: #f5f5f5;
  width: 100%;
}

.control {
  margin-top: 30rpx;
  display: flex;
  justify-content: space-between;
}

.controlnumber {
  font-size: 27rpx;
}

.controlbtn {
  display: flex;
  justify-content: space-between;
}

.btn {
  margin: 0 20rpx;
  width: 130rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  background-color: #fe412e;
  color: #fff;
  font-size: 25rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.btncolor {
  background-color: #d6d6d6;
}

.nostates {
  height: calc(100% - 75rpx - 1rpx);
  display: flex;
  margin-top: 30%;
  flex-direction: column;
  align-items: center;
  
}

.nostates image {
  width: 184rpx;
  height: 200rpx;
}

.nostatestitle {
  font-size: 28rpx;
  color: #323232;
  margin: 30rpx auto 50rpx;
}
