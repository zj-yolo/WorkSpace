.top_add {
  display: flex;
  flex-direction: row;
  margin: 20rpx;
  align-items: center;
}
.top_add .add_text {
  width: 90%;
  font-size: 28rpx;
}

.top_add .add_text view {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 48rpx;
  color: #666;
}

.top_add .add_iconB {
  width: 10%;
  text-align: center;
}

.top_add .add_iconB image {
  width: 30rpx;
  height: 30rpx;
}

.orderall {
  width: 100%;
  background-color: #fbfbfb;
}

.goodsDetail {
  font-size: 28rpx;
  width: 94%;
  margin-left: 3%;
  background-color: white;
}

.haha {
  display: flex;
  justify-content: space-between;
  margin: 0rpx 20rpx;
}

.goodsDetail .gs_left {
  width: 30%;
}

.goodsDetail .gs_left image {
  width: 140rpx;
  height: 140rpx;
}

.goodsDetail .gs_rig {
  width: 70%;
}

.goodsDetail .gs_rig .title {
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 10rpx;
}

.huibao {
  font-size: 24rpx;
  width: 94%;
  margin-left: 3%;
  background-color: white;
}

.huibao view {
  margin: 0rpx 20rpx;
  line-height: 60rpx;
}

.gs_rig .price {
  display: flex;
  justify-content: space-between;
  margin-top: 40rpx;
  color: red;
}

.yunfei {
  margin: 20rpx 5rpx;
  font-size: 28rpx;
  width: 94%;
  margin-left: 3%;
  background-color: white;
}

.yunfei view {
  padding: 10rpx 0rpx;
}

.liuyan {
  margin: 0rpx 5rpx 20rpx 5rpx;
  font-size: 28rpx;
  width: 94%;
  margin-left: 3%;
  background-color: white;
  padding-bottom: 20rpx;
}

.liuyan view {
  margin: 0rpx 20rpx;
  line-height: 60rpx;
}

.liuyan .area {
  height: 100rpx;
  margin-top: 10rpx;
  border: solid 1px #d1d1d1;
  width: 94%;
}

.fenxian {
  margin: 20rpx 5rpx;
  font-size: 28rpx;
  width: 94%;
  margin-left: 3%;
  background-color: white;
}

.fenxian .shuoming {
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 20rpx 20rpx;
  padding-top: 20rpx;
}

.fenxian .content {
  margin: 0rpx 20rpx;
  color: #666;
}

.checkbox {
  font-size: 28rpx;
  margin: 10rpx 5rpx;
}

.pay {
  font-size: 28rpx;
  margin: 10rpx 5rpx;
}

.footer {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  font-size: 30rpx;
  line-height: 100rpx;
  justify-content: space-between;
  display: flex;
  align-items: center;
  text-align: center;
  z-index: 10;
  background-color: white;
}

.footer view {
  width: 50%;
  height: 100%;
  text-align: center;
}

.footer_two {
  margin: 0rpx 20rpx;
  justify-content: space-between;
  display: flex;
  align-items: center;
  text-align: center;
}

.liji {
  background: linear-gradient(to right, red, red);
  color: white;
  height: 100rpx;
  font-size: 28rpx;
  margin-right: 0rpx;
  width: 30%;
  line-height: 100rpx;
  border-radius: 0rpx;
}

.boxview {
  display: flex;
  align-items: center;
  margin-top: 20rpx;
}

.tishi {
  text-align: center;
  margin-top: 50%;
  font-size: 28rpx;
}

.zhankai {
  border: solid 1px #908f8f;
  padding: 5rpx 20rpx;
  border-radius: 30rpx;
}

.textarea {
  padding: 20rpx 30rpx;
}
