.menu {
  background-color: #f5f5f5;
  display: flex;
  width: 100%;
  box-sizing: border-box;
  white-space: nowrap;
  overflow-y: scroll;
}

.menuItem {
  margin: 0 20rpx;
  padding: 20rpx 0;
  font-size: 28rpx;
  color: #b1b1b1;
}

.menuActive {
  font-size: 30rpx;
  color: #2f2f2f;
  border-bottom: 4rpx solid #2f2f2f;
  box-sizing: border-box;
}

.list {
  padding: 30rpx;
}

.ItemList {
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
  width: 100%;
}

.Imgs {
  width: 185rpx;
  height: 130rpx;
  padding: 10rpx;
  border: 1rpx solid #eee;
  box-sizing: border-box;
}

.content {
  margin-left: 30rpx;
  flex: 1;
}

.title {
  font-size: 30rpx;
  font-weight: 800rpx;
  color: #2f2f2f;
}

.subtitle {
  margin-top: 10rpx;
  font-size: 25rpx;
  color: #9b9b9b;
  display: -webkit-box;
  text-overflow: ellipsis;
  overflow: hidden;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
.noenterprise {
		display: flex;
		flex-direction: column;
		align-items: center;
		justify-content: center;
		margin-top: 20%;
	}

	.noImgs {
		width: 200rpx;
		height: 177rpx;
	}

	.texts {
		margin-top: 20rpx;
		font-size: 28rpx;
		color: #CCCCCC;
	}