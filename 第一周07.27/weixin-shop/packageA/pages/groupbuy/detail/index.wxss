  align-items: center;
  /* border-top: 1rpx solid #f3f3f3 */
}

.footer .item {
  width: 30%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  color: #fff
}

.rline {
  width: 100%;
  height: 25rpx;
  background-color: #f5f5f5
}

.select_price {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  font-size: 26rpx;
  margin: 0 20rpx;
  align-items: center;
  padding: 15rpx 0;
  min-height: 60rpx
}

.s_title {
  width: 90%
}

.s_img {
  width: 16rpx;
  height: 32rpx
}

.box {
