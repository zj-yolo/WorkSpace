}

.desc {
  font-size: 26rpx;
  margin: 10rpx 0 0 60rpx;
  width: 60%
}

.mb10 {
  margin-bottom: 18rpx
}

.red {
  color: red
}

.select_F {
  height: 40rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}

.t_sc {
  width: 45rpx;
  height: 45rpx;
  margin-left: -60rpx;
  margin-top: 18rpx
}

.bd {
  width: 100%;
  max-height: 760rpx;
  overflow-y: scroll;
  position: relative;
  z-index: 55
}

.list {
  margin-top: 30rpx;
  margin-left: 20rpx;
  font-size: 28rpx;
  padding-bottom: 25rpx;
  /* border-bottom: 1rpx solid #e5e5e5 */
}

.category {
  font-weight: bolder;
  margin-bottom: 20rpx
}

.type {
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  width: 95%
}

.item_F {
  background-color: #fff;
  padding: 10rpx 30rpx;
  border: 1rpx solid #e5e5e5;
  color: #e5e5e5;
  margin-right: 15rpx;
  margin-bottom: 20rpx;
  border-radius: 10rpx
}

.ft {
  height: 80rpx;
  width: 100%;
  position: absolute;
  bottom: 0;
  line-height: 80rpx;
  color: #fff;
  display: flex;
  flex-direction: row;
  font-size: 24rpx;
  text-align: center;
  z-index: 99
}

.ft_l {
  display: inline-block;
  width: 50%;
  background: -webkit-gradient(linear, left 0, right 0, from(#ffc700), to(#f99a00));
  border: 0;
  border-radius: 0;
  color: #fff;
  line-height: 80rpx;
  -webkit-appearance: none
}

.ft_l::after {
  content: none !important
}

.ft_r {
  display: inline-block;
  width: 50%;
  background: -webkit-gradient(linear, left 0, right 0, from(#fe7701), to(#ff4b00))
}

.view-btns-text-normal {
  color: #4d4d4d
}

.view-btns-text-select {
  /* color: #fff;
  background: #fb4d00 */
  border: 1rpx solid #ea1414;
  color: #ea1414;
}

.footerB {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  position: absolute;
  bottom: 0;
  display: flex;
  flex-direction: row;
  z-index: 2;
}

.footerB .left {
  width: 26%;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  /* border-top: 1rpx solid #f3f3f3; */
}

.footerB .item {
  width: 37%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 26rpx;
  color: #fff;
}

.swiperView {
  position: relative;
}

.dots {
  position: absolute;
  bottom: 20rpx;
  right: 20rpx;
  padding: 2rpx 14rpx;
  min-width: 70rpx;
  box-sizing: border-box;
  text-align: center;
  border: 2rpx solid #d4d4d4;
  color: #000;
  background: #fff;
  font-size: 26rpx;
}

.images {
  position: relative;
  background: #fff;
}

.swiper-image {
  width: 100%;
  height: 100%;
  background: #fff;
}

.priceWrap {
  background-color: #ea1414;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  color: #fff;
}

.groupNum {
  border: 1rpx solid #fff;
  border-radius: 5rpx;
  font-size: 18rpx;
  color: #fff;
  padding: 5rpx 8rpx;
}

.goodsprice {
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center
}

.allprice {
  vertical-align: bottom;
}

.titlewrap {
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.shareImgs {
  width: 50rpx;
  height: 50rpx;
}

.goodstitle {
  font-size: 28rpx;
  width: 85%;
}

.handleText {
  font-size: 28rpx;
  /* color: #767676; */
}

.handletype {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 30rpx;
}

.arrow {
  width: 20rpx;
  height: 28rpx;
}

.group {
  padding-left: 30rpx;
}

.groupTitle {
  padding: 35rpx 0;
  border-bottom: 1rpx solid #ececec;
}

.groupItem {
  height: 130rpx;
  padding-right: 20rpx;
  border-bottom: 1rpx solid #ececec;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.userIcon {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
}

.groupLeft {
  display: flex;
  align-items: center;
  flex-direction: row;
}

.groupRight {
  display: flex;
}

.groupneed {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.groupButton {
  background-color: #ea1414;
  font-size: 28rpx;
  color: #fff;
  height: 50rpx;
  width: 130rpx;
  text-align: center;
  line-height: 50rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  margin-left: 20rpx;
}
.towechat{
 width: 50%;
 text-align: center;
}
.wechatBorder{
  border-right: 1rpx solid #e5e5e5;
  border-left: 1rpx solid #e5e5e5;
}
button{
  margin: 0;
  padding: 0;
  background-color: #fff;
  border: none;
  line-height: normal;
}
button::after {
  border: none;
}
