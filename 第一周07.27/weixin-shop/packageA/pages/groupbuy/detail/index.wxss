page {
  width: 100%;
  height: auto
}

.wrap {
  padding: 0 20rpx;
  background-color: #fff;
  margin-bottom: 16rpx
}

.title {
  display: flex;
  flex-direction: row;
  margin-top: 28rpx;
  line-height: 44rpx
}

.tip {
  background-color: #fe3297;
  font-size: 28rpx;
  padding: 0 10rpx;
  border-radius: 4rpx;
  margin-right: 28rpx;
  color: #fff;
  height: 44rpx;
  flex-shrink: 0
}

.desc {
  margin-top: 30rpx;
  color: #c5c5c5;
  font-size: 28rpx
}

.sub_wrap {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  margin-top: 24rpx;
  font-size: 26rpx;
  color: #c5c5c5;
  margin-bottom: 32rpx
}

.tishi {
  padding: 16rpx 20rpx;
  background-color: #fff;
  color: #a0a0a0;
  font-size: 26rpx;
  line-height: 36rpx;
  margin-bottom: 26rpx
}

.footer {
  width: 100%;
  height: 100rpx;
  background-color: #fff;
