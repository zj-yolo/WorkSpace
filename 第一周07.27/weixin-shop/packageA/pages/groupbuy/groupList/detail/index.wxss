page {
  width: 100%;
  height: 100%
}

.wrap {
  padding: 0 20rpx
}

.i_store {
  display: flex;
  flex-direction: row;
  background-color: #fff;
  justify-content: flex-start;
  align-items: center
}

.is_left {
  display: flex;
  flex-direction: row;
  align-items: flex-start;
}

.is_right {
  display: flex;
  justify-content: flex-end
}

.i_img {
  width: 200rpx;
  height: 200rpx;
  background-color: #fff;
  margin: 20rpx;
  border-radius: 10rpx;
  flex-shrink: 0
}

.i_dataTitle {
  font-size: 28rpx;
  width: 52%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.groupnum {
  margin-top: 10rpx;
  display: flex;
  align-items: center;
}

.groupImg {
  width: 30rpx;
  height: 22rpx;
}

.i_group {
  margin-left: 10rpx;
  font-size: 20rpx;
  color: #b1b1b1;
}

.mt20 {
  margin-top: 20rpx;
  font-weight: bold;
  font-size: 28rpx;
  display: flex;
  flex-direction: column
}

.i_price {
  font-size: 35rpx;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  margin-right: 20rpx;
  color: #8e8e8e;
  margin-top: 80rpx
}

.oldprice {
  margin-left: 10rpx;
  font-size: 26rpx;
  color: #989898;
  text-decoration: line-through;
}

.w_touxiang_team {
  position: relative;
}

.tuanzhang {
  position: absolute;
  left: 50%;
  transform: translateX(-50%);
  bottom: 0;
  background-color: #fe5848;
  width: 60rpx;
  height: 30rpx;
  text-align: center;
  line-height: 30rpx;
  font-size: 18rpx;
  color: #fff;
}

.moregoodswrap {
  margin: 20rpx 0;
  width: 100%;
  padding: 0 30rpx;
  box-sizing: border-box;
}

.seemoreGoods {
  background: #ea1414;
  text-align: center;
  height: 88rpx;
  line-height: 88rpx;
  color: #fff;
  font-size: 25rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
}

.arrowImg {
  width: 20rpx;
  height: 20rpx;
}

.group_explain {
  display: flex;
  align-items: center;
  justify-content: space-around;
  font-size: 22rpx;
  margin: 10rpx 30rpx 30rpx;
  box-sizing: border-box;
}

.red {
  color: #ef4f4f
}

.w_time {
  padding: 40rpx 0;
  font-size: 26rpx;
  text-align: center
}

.w_touxiang {
  display: flex;
  justify-content: center;
  padding: 20rpx 0
}

.w_touxiang image {
  width: 90rpx;
  height: 90rpx;
  background-color: #fff;
  border-radius: 50%;
  margin: 0 6rpx
}

.w_tip {
  font-size: 26rpx;
  padding: 20rpx 0;
  text-align: center;
  color: #959595
}

.groupBottom {
  position: fixed;
  bottom: 0;
  left: 0;
  height: 100rpx;
  background: #fff;
  width: 100%;
  display: flex;
  align-items: center;
}
.bottomItem{
  text-align: center;
  line-height: 100rpx;
  width: 50%;
  color: #fff;
  background: #ea1414;
}

.cantuan {
  width: 70%;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  text-align: center;
  background-color: red;
  color: #fff;
  border-radius: 12rpx;
  margin: 20rpx auto
}

.line {
  height: 20rpx;
  background-color: #f3f3f3
}

.top {
  height: 80rpx;
  display: flex;
  flex-direction: row;
  text-align: center;
  line-height: 80rpx;
  font-size: 28rpx;
  margin-bottom: 20rpx
}

.t_item {
  width: 50%;
  height: 100%
}

.b_bottom {
  border-bottom: 1rpx solid red
}

.s_item {
  display: flex;
  justify-content: space-between;
  height: 90rpx;
  line-height: 90rpx;
  font-size: 24rpx;
  margin: 0 20rpx;
  border-bottom: 1rpx solid #f3f3f3
}

.si_top {
  display: flex;
  flex-direction: row;
  align-items: center
}

.si_img {
  width: 75rpx;
  height: 75rpx;
  background-color: #fff;
  border-radius: 50%;
  margin-right: 20rpx
}

.store {
  padding: 24rpx;
  display: flex;
  justify-content: space-between;
  font-size: 30rpx
}

.row1 {
  display: flex;
  flex-direction: row
}

.fangzi {
  width: 15rpx;
  height: 30rpx;
  background-color: red;
  margin-right: 10rpx
}

.foot {
  display: flex;
  flex-direction: row;
  margin-bottom: 20rpx;
  font-size: 26rpx
}

.pintuan {
  border-top: 1rpx solid #f3f3f3;
  border-bottom: 1rpx solid #f3f3f3;
  background-color: #fff;
  margin-bottom: 120rpx;
  font-size: 28rpx;
  width: 750rpx;
  margin-top: 30rpx
}

.p_item {
  width: 25%;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center
}

.num {
  margin-right: 16rpx
}

.p_select {
  color: #f0373d
}

.richtext {
  text-align: center;
  margin-bottom: 120rpx;
  width: 100%;
  height: 100%
}

.f_bottom {
  display: flex;
  flex-direction: row;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  width: 100%
}

.f_left {
  width: 80%;
  line-height: 100rpx;
  font-size: 28rpx;
  text-align: center;
  color: #fff;
  background-color: #fd5454;
  border-radius: 0
}

.f_right {
  width: 20%;
  background-color: #333;
  font-size: 24rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  text-align: center
}

.uactives {
  display: none
}

.actives {
  display: block
}

.haha {
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  z-index: 99999;
  margin-left: 84%;
  margin-top: 900rpx;
  box-shadow: 0rpx 0rpx 10rpx 10rpx #e3e3e3;
  border-radius: 50%;
}

.haha image {
  width: 100rpx;
  height: 100rpx;
}