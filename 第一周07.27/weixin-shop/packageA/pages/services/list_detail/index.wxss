page{width: 100%;height: 100%;}
.view-order image{width:50rpx;height:50rpx;margin-left:20rpx}
.view-order{font-family:"Times New Roman";display:flex;width:100%;flex-direction:column;line-height:60rpx;font-size:29rpx;}
.view-address{display:flex;flex-direction:column;justify-content:space-between;width:100%;background:#3AA0B7;border-bottom:1px solid #eee}
.view-addressleft{display:flex;flex-direction:row;padding:20rpx ;justify-content:flex-start;align-items:center;}
.view-addressleft image{width:50rpx;height:50rpx}
.view-addressleft-info{display:flex;flex-direction:column;margin-left:20rpx;justify-content:space-around;align-items:flex-start;height:85rpx}
.text-name{font-size:29rpx;color:#444}
.text-address{font-size:29rpx;color:#444}
.text_between{display:flex;flex-direction:row;justify-content:space-between;height:64rpx;line-height:64rpx;font-size:25rpx;margin:0 20rpx}
.text_order{display:flex;flex-direction:column;height:64rpx;line-height:64rpx;font-size:25rpx;margin:0 20rpx}
.seller{display:flex;flex-direction:column;height:auto;line-height:64rpx;font-size:29rpx;margin:0 20rpx}
.view-addressright{height:50rpx;margin-right:20rpx;display:flex;flex-direction:row;align-items:center}
.text-mobile{font-size:29rpx;color:#444}
.view-addressright image{width:50rpx;height:50rpx}
.view-title{display:flex;flex-direction:row;height:70rpx;line-height:70rpx;margin-left:20rpx;align-items:center}
.view-title image{width:40rpx;height:40rpx}
.view-title text{font-size:29rpx;color:#000}
.class-line{width:710rpx;margin-left:20rpx;height:2rpx;background:#F4F4F4}
.container{display:flex;flex-direction:column;align-items:center;justify-content:space-between;box-sizing:border-box;}
.carts-list{display:flex;flex-direction:column;padding:0 20rpx;border-bottom:1px solid #eee;}
.carts-item{display:flex;flex-direction:row;height:150rpx;width:100%;padding:20rpx 0;justify-content:space-between;border-color: #fff;}
.carts-image{width:150rpx;height:150rpx;border-width:3rpx;border-style:solid;border-color:#ccc}
.carts-text{margin-left:10rpx;display:flex;flex-direction:column;}
.carts-right{height:100rpx;display:flex;flex-direction:row;justify-content: space-between;}
.carts-title{font-size:29rpx;color:#444;line-height:38rpx;display:-webkit-box;overflow:hidden;-webkit-box-orient:vertical;-webkit-line-clamp:2}
.carts-key-name{margin-top:10rpx;font-size:27rpx;color:#999}
.carts-subtitle{font-size:25rpx;color:#a9a9a9;padding:0 20rpx;display:flex;flex-direction:row;justify-content:space-between}
.carts-price{color:#f60;font-size:14px;line-height:22px;margin-top:12rpx}
.stepper{border:1px solid #ccc;border-radius:3px;width:80px;height:26px;margin:0 auto}
.stepper text{width:19px;line-height:26px;text-align:center;float:left}
.stepper input{color:#000;float:left;margin:0 auto;width:40px;height:26px;text-align:center;font-size:12px;border-left:1px solid #ccc;border-right:1px solid #ccc}
.stepper
.normal{color:#000}
.stepper
.disabled{color:#ccc}
.carts-list icon{margin-top:60rpx;margin-right:20rpx;}
.carts-footer{width:100%;height:80rpx;display:flex;flex-direction:row;justify-content:space-between;align-items:center;position:fixed;bottom:0;border-top:1px solid #eee;background:#fff}
.carts-footer icon{margin-left:20rpx;margin-top:20rpx}
.carts-footer text{font-size:30rpx;margin-left:8rpx;line-height:10rpx}
.carts-footer
.button{line-height:80rpx;text-align:center;width:220rpx;height:80rpx;background-color:#f60;color:#fff;font-size:36rpx;border-radius:0;border:0}
.carts-footer-left{display:flex;flex-direction:row;align-items:center}
.carts-footer-left text{margin-top:20rpx}
.text-price{font-size:25rpx;}
.text-red{font-size:25rpx;}
.image-delete{width:50rpx;height:60rpx}
.text_price{font-size:25rpx;}
.class-line1{width:750rpx;height:20rpx;background:#F4F4F4}
.view-price{display:flex;flex-direction:column;justify-content:flex-start;align-items:flex-end}
.view-price1{margin-top:30rpx;margin-right:20rpx;display:flex;flex-direction:row;justify-content:flex-end;align-items:center}
.text-price1{font-size:29rpx;color:grey}
.text-save{width:680rpx;align-self:center;font-size:32rpx;margin-top:30rpx;color:#fff;text-align:center;height:70rpx;border-radius:9rpx;background:orange;line-height:70rpx}
.parent{display:flex;flex-direction:column;width:100%}
.Order{display:flex;flex-direction:row-reverse}
.Order view{width:200rpx;height:60rpx;line-height:60rpx;margin-right:20rpx;font-size:30rpx;text-align:center;margin-top:20rpx;border-radius:8rpx}
.btn{font-size: 29rpx;margin:20rpx;border:1rpx solid #e5e5e5; padding:10rpx 30rpx;width: 140rpx;text-align: center;border-radius: 12rpx;}
.dn{display: none;}

.pay_status{
	line-height: 80rpx;font-size: 25rpx;background-color: #F5F5F5;padding:0 20rpx;
}
.store_name{
	line-height: 80rpx;font-size: 29rpx;padding:0 20rpx;
}
.store_addr{
	line-height: 80rpx;font-size: 25rpx;padding:0 20rpx;
}
.btn_wrap{
	display: flex;justify-content: flex-end;
}
.btn_item{
	font-size: 25rpx;line-height: 35rpx;padding:10rpx 30rpx;border:1rpx solid #959595;color:#959595;border-radius: 35rpx;margin: 30rpx 30rpx 0 0 ;
}