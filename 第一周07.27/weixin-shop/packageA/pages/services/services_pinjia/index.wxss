@import "../../../../pages/order/details/index.wxss";


.page{background-color:#fff;}
.carts-list{padding-left:0}
.view-title{margin-top:20rpx;background-color:#fff;padding:0 20rpx;box-sizing:border-box;display:flex;flex-direction:row;font-size:26rpx}
.view-title image{margin-right:30rpx}
.carts-item-yuyue{display:flex;flex-direction:row;width:100%;border-bottom:1px solid #eee;justify-content:space-between;padding:20rpx 20rpx;box-sizing:border-box}
.ushow{display:none}
.show{display:block}
.gallery{display:flex;flex-direction:row;justify-content:center;align-items:center;flex-wrap:wrap}
.formTitle{height:84rpx;line-height:64rpx;margin-left:30rpx;font-size:30rpx}
.formTitle text{display:inline-block;color:#666;margin-left:300rpx}
.item{display:flex;flex-direction:row;justify-content:center;align-items:center;position:relative;margin-right:20rpx;margin-bottom:10rpx}
.delete{position:absolute;height:30rpx;bottom:0;width:100%;background:#ccc;opacity:.8;z-index:2}
.delete image{position:absolute;bottom:0;width:30rpx;height:30rpx;z-index:1}
.button-upload{width:150rpx;height:150rpx}
.upload{margin:20rpx 20rpx 0;height:80rpx;line-height:80rpx}
.carts-right{margin-right:0}
.pinjia_yuyue{position:fixed;bottom:0;height:80rpx;line-height:80rpx;z-index:9;width:100%;border-radius: 0;font-size: 25rpx;color:#fff;}

.carts-text {
	width:100%;margin:0;margin-left: 30rpx;
}
.carts-title{
	height: 50rpx;line-height: 50rpx;font-size: 27rpx;
}

.carts-image{
	width: 130rpx;height: 130rpx;border-radius: 12rpx;border-color: #fff;
}

.ping{
	height: 70rpx;line-height: 70rpx;font-size: 25rpx;display: flex;flex-direction: row;
}

.d_img{
	width:40rpx;height: 40rpx;margin:0 20rpx 0 0 ;
}

.d_item{
	line-height: 70rpx;margin: 0 20rpx 0 0 ;position: relative;display: flex;align-items: center;
}

.demo{line-height: 50rpx;}