.banner image {
  height: 365rpx;
  width: 100%;
}

.formInput {
  padding: 0 30rpx;
}

.label {
  margin: 35rpx auto 25rpx;
  color: #565656;
  font-size: 28rpx;
}

.changeplace {
  position: relative;
}

.details {
  color: #c0c0c0;
  font-size: 28rpx;
}

.arrowRight {
  width: 42rpx;
  height: 42rpx;
  position: absolute;
  right: 28rpx;
  top: 14rpx;
}

input, .place, .changeplace {
  line-height: 68rpx;
  border: 1rpx solid #c0c0c0;
  width: 100%;
  height: 68rpx;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding-left: 30rpx;
  font-size: 28rpx;
}

textarea {
  border: 1rpx solid #c0c0c0;
  width: 100%;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding: 20rpx;
  font-size: 28rpx;
}

.sumbitBtn {
  height: 68rpx;
  line-height: 68rpx;
  background-color: #dc1414;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
  border-radius: 10rpx;
  margin: 40rpx auto 50rpx;
}
