page{
	background-color: #e5e5e5;
}
@font-face {font-family:"HMfont-home";src:url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');}
.icon {
  margin: 0;
  font-family:"HMfont-home" !important;
  font-size:56rpx;
  font-style:normal;
  color: #333;
}
.icon.biaoqing:before {
  content: "\e797";
}
.icon.jianpan:before {
  content: "\e7b2";
}
.icon.yuyin:before {
  content: "\e805";
}
.icon.tupian:before {
  content: "\e639";
}
.icon.chehui:before {
  content: "\e904";
}
.icon.luyin:before {
  content: "\e905";
}
.icon.luyin2:before {
  content: "\e677";
}
.icon.other-voice:before {
  content: "\e667";
}
.icon.my-voice:before {
  content: "\e906";
}
.icon.hongbao:before {
  content: "\e626";
}
.icon.tupian2:before {
  content: "\e674";
}
.icon.paizhao:before {
  content: "\e63e";
}
.icon.add:before {
  content: "\e655";
}
.icon.close:before {
  content: "\e607";
}
.icon.to:before {
  content: "\e675";
}
.hidden{
	display: none !important;
}
.popup-layer{
	transition: all .15s linear;
	width: 96%;
	height: 42vw;
	padding: 20rpx 2%;
	background-color: #f3f3f3;
	border-top: solid 1rpx #ddd;
	position: fixed;
	z-index: 20;
	top: 100%;
}
.popup-layer.showLayer{
  transform: translate3d(0,-42vw,0);
}
.emoji-swiper{
  height: 40vw;
}
.emoji-swiper	swiper-item{
  display: flex;
  align-content: flex-start;
  flex-wrap: wrap;
}
.emoji-swiper	swiper-item	view{
  width: 12vw;
  height: 12vw;
  display: flex;
  justify-content: center;
  align-items: center;
}
.emoji-swiper	swiper-item	view image{
  width: 8.4vw;
  height: 8.4vw;
}
.more-layer{
  width: 100%;
  height: 42vw;
}
.list{
  width: 100%;
  display: flex;
  flex-wrap: wrap;
}
.more-layer .box{
  width: 18vw;
  height: 18vw;
  border-radius: 20rpx;
  background-color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 3vw 2vw 3vw;
}
.box .icon{
  font-size: 70rpx;
}
.input-box{
	width: 98%;
	min-height: 100rpx;
	padding: 0 1% 20rpx;
	background-color: #f2f2f2;
	display: flex;
	position: fixed;
	z-index: 20;
	bottom:-2rpx;
  transition: all .15s linear;
	border-bottom: solid 1rpx #ddd;
}
.input-box.showLayer{
  transform: translate3d(0,-42vw,0);
}
.voice,.more{
  flex-shrink: 0;
  width: 90rpx;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.send{
  flex-shrink: 0;
  width: 100rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.btn{
  width: 90rpx;
  height: 56rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background:linear-gradient(to right,#f09b37,#eb632c);
  color: #fff;
  border-radius: 6rpx;
  font-size: 24rpx;
}
.textbox{
  width: 100%;
  min-height: 70rpx;
  margin-top: 15rpx;
}
.voice-mode{
  width: calc(100% - 2rpx);
  height: 68rpx;
  border-radius: 70rpx;
  border:solid 1rpx #cdcdcd;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
  background-color: #fff;
  color: #555;
}
.voice-mode.recording{
  background-color: #e5e5e5;
}
.text-mode{
  width: 100%;
  min-height: 70rpx;
  display: flex;
  background-color: #fff;
  border-radius: 40rpx;
}
.text-mode .box{
  width: 100%;
  padding-left: 30rpx;
  min-height: 70rpx;
  display: flex;
  align-items: center;
}
.box textarea{
  width: 100%;
}
.em{
  flex-shrink: 0;
  width: 80rpx;
  padding-left: 10rpx;
  height: 70rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.content{
	width: 100%;
}
.msg-list{
  width: 96%;
  padding: 0 2%;
  position: absolute;
  top: 0;
  bottom: 120rpx;
}
.loading{
  display: flex;
  justify-content: center;
}
@keyframes stretchdelay {
  0%, 40%, 100% {
    transform: scaleY(0.6);
  }
  20% {
    transform: scaleY(1.0);
  }
}
.spinner{
  margin: 20rpx 0;
  width: 60rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.spinner view {
  background-color: #f06c7a;
  height: 50rpx;
  width: 6rpx;
  border-radius: 6rpx;
  animation: stretchdelay 1.2s infinite ease-in-out;
}
.spinner .rect2 {
  animation-delay: -1.1s;
}
.spinner .rect3 {
  animation-delay: -1.0s;
}
.spinner .rect4 {
  animation-delay: -0.9s;
}
.spinner .rect5 {
  animation-delay: -0.8s;
}
.row{
  padding: 20rpx 0;
}
.row:first-child{
  margin-top: 20rpx;
}
.system{
  display: flex;
  justify-content: center;
}
.system view{
  padding: 0 30rpx;
  height: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #c9c9c9;
  color: #fff;
  font-size: 24rpx;
  border-radius: 40rpx;
}
.my .left,.other .right{
  width: 100%;
  display: flex;
}
.bubble{
  max-width: 70%;
  min-height: 50rpx;
  border-radius: 10rpx;
  padding: 15rpx 20rpx;
  display: flex;
  align-items: center;
  font-size: 32rpx;
  word-break: break-word;
}
.bubble.img{
  background-color: transparent;
  padding:0;
  overflow: hidden;
}
.bubble.img image{
  max-width: 350rpx;
  max-height: 350rpx;
}
.status{
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  font-size: 30rpx;
}
.statusTitle{
  margin-bottom: 8rpx;
  font-size: 30rpx;
  line-height: 1.6;
}
.moreText{
  margin-bottom: 20rpx;
}
.contact{
  display: flex;
  margin-bottom: 20rpx;
  padding: 16rpx;
  width: 100%;
  font-size: 28rpx;
  align-items: center;
  background: #efeff4;
  border-radius: 4px;
  box-sizing: border-box;
}
.contact:last-child{
  margin: 0;
}
.contactImg{
  margin-right: 14rpx;
  width: 32rpx;
  height: 32rpx;
}
.vrImg{
  margin-right: 14rpx;
  width: 40rpx;
  height: 25rpx;
}
.my .right,.other .left{
  flex-shrink: 0;
  width: 80rpx;
  height: 80rpx;
}
.my .right image,.other .left	image{
  width: 80rpx;
  height: 80rpx;
  border-radius: 10rpx;
}
.my{
  width: 100%;
  display: flex;
  justify-content: flex-end;
}
.left{
  min-height: 80rpx;
  align-items: center;
  justify-content: flex-end;
}
.my .bubble{
  background-color: #f06c7a;
  color: #fff;
}
.my .right{
  margin-left: 15rpx;
}
.other{
  width: 100%;
  display: flex;
}
.other .left{
  margin-right: 15rpx;
}
.other .right{
  flex-wrap: wrap;
}
.other .username{
  width: 100%;
  height: 45rpx;
  font-size: 24rpx;
  color: #999;
  display: flex;
}
.other .name{
  margin-right: 50rpx;
}
.other .bubble{
  background-color: #fff;
  color: #333;
}