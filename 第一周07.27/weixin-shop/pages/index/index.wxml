<template name="map">
	<view class="mapAssembly">
		<map id="map" longitude="{{data.style.longitude}}" latitude="{{data.style.latitude}}" scale="14" controls="{{controls}}" bindcontroltap="controltap" data-longitude='{{data.style.longitude}}' data-latitude='{{data.style.latitude}}' markers="{{markers}}" show-location style="width: 100%; height: 300px;"></map>
	</view>
</template>
<!-- 团购组 -->
<template name="begroups">
	<block wx:if="{{dataGroup.params.hideborder == 0}}">
		<view style="background-color:{{dataGroup.style.background}};margin-top:{{dataGroup.style.margintop}}rpx;border:solid 1px {{dataGroup.style.timebordercolor}}">
			<view style='margin:0 20rpx;display:flex;justify-content:space-between;align-items:center;text-align: center;height:100rpx;line-height:100rpx;' class='groupBe'>
				<image src="{{dataGroup.params.iconurl}}" style='width:100rpx;height:30rpx;'></image>
				<text style='font-size:30rpx;color:{{dataGroup.style.morecolor}};' bindtap='intoGroupList'>查看更多 》</text>
			</view>
			<scroll-view class="list_store" scroll-x="true">
				<block wx:if="{{dataList}}">
					<block wx:key wx:for="{{dataList}}">
						<view class="itemTwo" id='{{item.id}}' bindtap="toGroupDetail" style="background-color:{{dataGroup.style.background}};">
							<image lazy-load="true" src="{{item.thumb}}" style="width: 100%;height: 100%;"></image>
							<view style='color:{{dataGroup.style.marketpricecolor}};' class='groupprice'>￥{{item.groupsprice}}</view>
							<view style='color:{{dataGroup.style.productpricecolor}};' class='nowPrice'>￥{{item.price}}</view>
						</view>
					</block>
				</block>
				<block wx:else>
					<view style='text-align: center;height:60rpx;line-height:60rpx;'>没有团购商品</view>
				</block>
			</scroll-view>
		</view>
	</block>
	<block wx:else>
		<view style="background-color:{{dataGroup.style.background}};margin-top:{{dataGroup.style.margintop}}rpx;">
			<view style='margin:0 20rpx;display:flex;justify-content:space-between;align-items:center;text-align: center;height:100rpx;line-height:100rpx;' class='groupBe'>
				<image src="{{dataGroup.params.iconurl}}" style='width:100rpx;height:30rpx;'></image>
				<view bindtap='intoGroupList'>
					<text style='font-size:30rpx;color:{{dataGroup.style.morecolor}};'>查看更多</text>
					<image style='width:17rpx;height:20rpx;margin-left:10rpx;' src='http://tws.cnweisou.com/images/rightArrow.png'></image>
				</view>
			</view>
			<scroll-view class="list_store" scroll-x="true">
				<block wx:if="{{dataList}}">
					<block wx:key wx:for="{{dataList}}">
						<view class="itemTwo" id='{{item.id}}' bindtap="toGroupDetail" style="background-color:{{dataGroup.style.background}};">
							<image lazy-load="true" src="{{item.thumb}}" style="width: 100%;height: 100%;"></image>
							<view style='color:{{dataGroup.style.marketpricecolor}};' class='groupprice'>￥{{item.groupsprice}}</view>
							<view style='color:{{dataGroup.style.productpricecolor}};' class='nowPrice'>￥{{item.price}}</view>
						</view>
					</block>
				</block>
				<block wx:else>
					<view style='text-align: center;height:60rpx;line-height:60rpx;'>没有团购商品</view>
				</block>
			</scroll-view>
		</view>
	</block>
</template>
<!-- NEWS4的模板-->
<template name="new4">
	<view class="container">
		<view class="top" style="height:{{date.topHeight}}px;line-height:{{date.topHeight}}px;">
			<view class="cover {{isClick == true?'showcover':''}}">
				<scroll-view scroll-y class="coverscro" bindtap="clickContAgain">
					<text class="titleCover">
					{{date.test.shopinfo.name}}
					</text>
					<text class="contCover">
					{{date.test.shopinfo.description}}
					</text>
				</scroll-view>
			</view>
			<image class="brand" src="{{date.test.shopinfo.logo}}"></image>
			<view class="info" bindtap="clickCont">
				<view class="title1">
					<text>{{date.test.shopinfo.name}}</text>
				</view>
				<view class="content">
					<text>{{date.test.shopinfo.description}}</text>
				</view>
			</view>
			<view class="phoneX" bindtap="click_phone">
				<image src="https://tws.cnweisou.com/images/Phone3.png"></image>
			</view>
		</view>
		<view class="body" style="height:{{date.swiperHeight}}px;padding-top:{{date.topHeight}}px;">
			<scroll-view scroll-y style="height:{{date.swiperHeight}}px;">
				<view class="titles" style="height:{{date.swiperHeight+300}}px;">
					<block>
						<view wx:if="{{date.test.category}}" wx:key wx:for="{{date.test.category}}" id="{{index}}" bindtap="show" class="title {{selectedTitle==index ? 'active' : ''}}">
							<view>
								<image style="width:42rpx;height:42rpx;border-radius:50%;margin:0px 10rpx;" src="{{item.advimg}}"></image>
							</view>
							<text>{{item.name}}</text>
						</view>
					</block>
				</view>
			</scroll-view>
			<scroll-view scroll-y style="height:{{date.swiperHeight}}px;">
				<view style="height:{{date.swiperHeight+300}}px;">
					<block wx:if="{{date.arr}}">
						<view class="right" wx:key wx:for="{{date.arr[selectedTitle]}}">
							<image class="repast" src="{{item.thumb}}"></image>
							<view class="descirbe" style="width:280rpx;height:180rpx;">
								<view class="describe_one">
									<text class="con_title">{{item.title}}</text>
								</view>
								<view class="describe_two">
									<text class="amount">已售{{item.sales}}</text>
								</view>
								<view class="describe_three">
									<text class="price">￥{{item.marketprice}}</text>
								</view>
							</view>
							<view class="shopping">
								<view class="circle add" bindtap="add" data-index="{{index}}" data-id="{{item.id}}">+</view>
								<view class="selectnum" wx:if="{{item.mytotal && item.mytotal > 0}}">
									<text class="selectNumber">{{item.mytotal}}</text>
								</view>
								<view class="circle delete circle1" bindtap="reduce" data-index="{{index}}" style="color:gray" wx:if="{{item.mytotal && item.mytotal > 0}}">-</view>
							</view>
						</view>
					</block>
				</view>
			</scroll-view>
		</view>
		<view class="shopCar" bindtap="getCarts">
			<view wx:if="{{Allnum}}" class="circleNum  shopCartI">{{Allnum}}</view>
			<text wx:if="{{Allnum}}">￥{{Allprice}}</text>
			<image src="https://tws.cnweisou.com/images/cart1.png"></image>
		</view>
	</view>
	<view class="orderForm dn {{showView?'db':''}} " style="z-index:100000;bottom:-30rpx;">
		<view class="shadow " catchtap="topShow"></view>
		<view class="formBox">
			<view class="orderTop">
				<image src="https://tws.cnweisou.com/images/delete.png"></image>
				<text bindtap="deleteCart">清空购物车</text>
			</view>
			<scroll-view scroll-y style="height:380rpx;">
				<block wx:key wx:for="{{selectGoods}}">
					<view class="list">
						<text class="listText">{{item.title}}</text>
						<text class="listPrice">￥{{item.marketprice}}</text>
						<view class="shopping listShopping">
							<view class="box">
								<view class="circle add" bindtap="allAdd" data-index="{{index}}">+</view>
							</view>
							<view class="numBox">
								<text class="shopNumber">{{item.mytotal}}</text>
							</view>
							<view class="circle delete circle1 " bindtap="allDelete" data-index="{{index}}" style="color:gray">-</view>
						</view>
					</view>
				</block>
			</scroll-view>
			<view class="listBottom">
				<text class="totalPrice">￥{{Allprice}}</text>
				<view class="btn" bindtap="bindCheckout">去下单</view>
			</view>
		</view>
	</view>
	<view class="bgtypeList" wx:if="{{isSecond}}">
		<scroll-view scroll-y class="cardAgainType">
			<view class="title">
				<view class="left">{{goods_name}}</view>
				<image src="https://tws.cnweisou.com/images/canyinError.png" bindtap="closeSecond"></image>
			</view>
			<view class="specifications" wx:key wx:for="{{goods_spec_list}}" data-myindex="{{index}}">
				<view class="specificationsTitle">{{item[index].spec_name}}：</view>
				<view class="specificationsCont" wx:for="{{item}}" wx:key data-index="{{index}}" data-spec_name="{{item.spec_name}}" bindtap="selectSpecif">
					<text class="{{item.isClick == 1?'selected':''}}">{{item.item}}</text>
				</view>
			</view>
			<view class="determineAgin">
				<text class="money">￥{{nowPrice}}</text>
				<text class="ok" bindtap="selectOk">选好了</text>
			</view>
		</scroll-view>
	</view>
</template>
<loading wx:if="{{loading}}"></loading>