page{
  background: #f4f4f4;
}
.tabs{
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #d2d2d2;
  background: #fff;
}
.tab{
  width: 140rpx;
  font-size: 30rpx;
  text-align: center;
}
.text-normal{
  display: inline-block;
  padding: 14rpx 20rpx;
}
.text-select{
  display: inline-block;
  padding: 14rpx 20rpx;
  color: #e50b0b;
  border-bottom: 3px solid #e50b0b;
}
.listItem{
  margin-bottom: 20rpx;
  padding: 26rpx 24rpx 30rpx;
  width: 100%;
  font-size: 30rpx;
  background: #fff;
  box-sizing: border-box;
}
.orderStatus{
  font-size: 28rpx;
  text-align: right;
  color: #e82c2c;
}
.commodity{
  display: flex;
  margin-bottom: 34rpx;
}
.commodityImg{
  margin-right: 14rpx;
  width: 168rpx;
  height: 168rpx;
}
.info{
  padding-top: 10rpx;
  width: calc(100% - 182rpx);
}
.titleContainer{
  display: flex;
  width: 100%;
  justify-content: space-between;
}
.titleContainer{
  margin-bottom: 10rpx;
}
.title{
  overflow: hidden;
  width: 80%;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.standard{
  width: 100%;
  font-size: 26rpx;
  color: #868686;
}
.price{
  text-align: right;
}
.operateContainer{
  display: flex;
  justify-content: flex-end;
}
.operateItem{
  width: 160rpx;
  height: 64rpx;
  line-height: 64rpx;
  font-size: 28rpx;
  text-align: center;
  border: 1px solid #939194;
  border-radius: 8rpx;
}
.common{
  margin-right: 20rpx;
  border-color: #939194;
}
.buy{
  background: #e50b0b;
  color: #fff;
  border-color: #e50b0b;
}
.comment{
  color: #e50b0b;
  border-color: #e50b0b;
}
.scrollTop{
  width:60rpx; 
  height:60rpx; 
  position:fixed; 
  bottom:30rpx; 
  right:30rpx; 
  z-index:10000; 
} 
.scrollTop image{
  width:60rpx; 
  height:60rpx; 
}
.noData {
  width: 100%;
  margin: 300rpx auto 0;
}
.noData image {
  width: 300rpx;
  height: 335rpx;
  margin: 0 auto;
  display: block;
}